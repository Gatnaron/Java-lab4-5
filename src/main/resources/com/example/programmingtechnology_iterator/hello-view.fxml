<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.programmingtechnology_iterator.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>


    <BorderPane fx:id="borderPane" prefHeight="200.0" prefWidth="200.0">
        <center>
        </center>
    </BorderPane>
    <GridPane>
        <Button mnemonicParsing="false" onAction="#startLoop_Button" prefHeight="26.0" prefWidth="48.0" text="▶" GridPane.columnIndex="1" />

        <Button mnemonicParsing="false" onAction="#endLoop_Button" prefHeight="26.0" prefWidth="52.0" text="⏸" GridPane.columnIndex="2" />
       <Button mnemonicParsing="false" onAction="#back_Button" prefHeight="26.0" prefWidth="56.0" text="←" />
       <Button mnemonicParsing="false" onAction="#forward_Button" prefHeight="26.0" prefWidth="50.0" text="→" GridPane.columnIndex="3" />
      <columnConstraints>
         <ColumnConstraints />
         <ColumnConstraints />
         <ColumnConstraints />
         <ColumnConstraints />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
    </GridPane>

</VBox>
